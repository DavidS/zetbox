<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<behaviors>
			<serviceBehaviors>
				<behavior name="HttpGetBehaviour">
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service name="Kistl.Server.KistlService"
				behaviorConfiguration="HttpGetBehaviour">
				<endpoint binding="wsHttpBinding"
					contract="Kistl.API.IKistlService" 
					address="http://localhost:6666/KistlService" />
			</service>
		</services>
	</system.serviceModel>
</configuration>