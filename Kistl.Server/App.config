<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="KistlContext" connectionString="metadata=res://*;provider=System.Data.SqlClient;provider connection string='Data Source=.\SQLEXPRESS;Initial Catalog=Kistl;Integrated Security=True;MultipleActiveResultSets=true;'" providerName="System.Data.EntityClient" />
    <add name="Kistl.Server.Properties.Settings.KistlDatabase" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Kistl;Integrated Security=True;MultipleActiveResultSets=true;" />
  </connectionStrings>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="HttpGetBehaviour">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="Kistl.Server.KistlService"
     behaviorConfiguration="HttpGetBehaviour">
        <endpoint binding="wsHttpBinding"
      contract="Kistl.API.IKistlService"
      address="http://localhost:6666/KistlService" />
      </service>
    </services>
  </system.serviceModel>
  <system.diagnostics>
    <trace>
      <listeners>
        <add name="tcp" type="TraceClient.TcpTraceListener, TraceListener" port="ServerAppDomain:667;666" />
        <add name="ConsoleListener" type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
