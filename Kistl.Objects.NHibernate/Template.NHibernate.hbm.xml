<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   default-cascade="save-update" >
    <class name="Kistl.App.GUI.TemplateNHibernateImpl+TemplateProxy, Kistl.Objects.NHibernateImpl"
           proxy="Kistl.App.GUI.TemplateNHibernateImpl+TemplateProxy, Kistl.Objects.NHibernateImpl"
           schema="`gui`"
           table="`Templates`"
           abstract="false"
           dynamic-update="false"
           optimistic-lock="none"
           batch-size="100">

        <id name="ID" column="`ID`" type="Int32" unsaved-value="0">
            <generator class="native">
                <param name="schema">`gui`</param>
                <param name="sequence">`Templates_ID_seq`</param>
            </generator>
        </id>

        <!-- define the properties -->
<!-- ObjectReferenceProperty -->
        <many-to-one name="DisplayedTypeAssembly" column="`fk_DisplayedTypeAssembly`" class="Kistl.App.Base.AssemblyNHibernateImpl+AssemblyProxy, Kistl.Objects.NHibernateImpl" />

        <!-- ValueTypeProperty isValue -->
        <property name="DisplayedTypeFullName" 
                  column="`DisplayedTypeFullName`" 
                  optimistic-lock="false"/>
        <!-- ValueTypeProperty isValue -->
        <property name="DisplayName" 
                  column="`DisplayName`" 
                  optimistic-lock="false"/>
<!-- ObjectReferenceProperty -->
        <!-- NMProperty -->
        <!-- rel=Template_hasMenu_Visual_RelationEntry -->
        <!-- relEnd=Template otherEnd=Menu -->
        <set name="Menu" table="`Template_hasMenu_Menu`" inverse="true" cascade="all-delete-orphan" batch-size="100" >
            <key column="`fk_Template`" />
            <one-to-many class="Kistl.App.GUI.Template_hasMenu_Visual_RelationEntryNHibernateImpl+Template_hasMenu_Visual_RelationEntryProxy,Kistl.Objects.NHibernateImpl" />
        </set>

<!-- ObjectReferenceProperty -->
        <many-to-one name="VisualTree" column="`fk_VisualTree`" class="Kistl.App.GUI.VisualNHibernateImpl+VisualProxy, Kistl.Objects.NHibernateImpl" />


        <!-- define the subclasses -->

    </class>
</hibernate-mapping>