<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   default-cascade="save-update" >
    <class name="Kistl.App.GUI.VisualNHibernateImpl+VisualProxy, Kistl.Objects.NHibernateImpl"
           proxy="Kistl.App.GUI.VisualNHibernateImpl+VisualProxy, Kistl.Objects.NHibernateImpl"
           schema="`gui`"
           table="`Visuals`"
           abstract="false"
           dynamic-update="false"
           optimistic-lock="none">

        <id name="ID" column="`ID`" type="Int32" unsaved-value="0">
            <generator class="native">
                <param name="schema">`gui`</param>
                <param name="sequence">`Visuals_ID_seq`</param>
            </generator>
        </id>

        <!-- define the properties -->
<!-- ObjectReferenceProperty -->
        <!-- NMProperty -->
        <!-- rel=Visual_contains_Visual_RelationEntry -->
        <!-- relEnd=Visual otherEnd=Children -->
        <set name="Children" table="`Visual_contains_Children`" inverse="true" cascade="all-delete-orphan" batch-size="100" >
            <key column="`fk_Visual`" />
            <one-to-many class="Kistl.App.GUI.Visual_contains_Visual_RelationEntryNHibernateImpl+Visual_contains_Visual_RelationEntryProxy,Kistl.Objects.NHibernateImpl" />
        </set>

<!-- ObjectReferenceProperty -->
        <!-- NMProperty -->
        <!-- rel=Visual_hasContextMenu_Visual_RelationEntry -->
        <!-- relEnd=Visual otherEnd=ContextMenu -->
        <set name="ContextMenu" table="`Visual_hasContextMenu_ContextMenu`" inverse="true" cascade="all-delete-orphan" batch-size="100" >
            <key column="`fk_Visual`" />
            <one-to-many class="Kistl.App.GUI.Visual_hasContextMenu_Visual_RelationEntryNHibernateImpl+Visual_hasContextMenu_Visual_RelationEntryProxy,Kistl.Objects.NHibernateImpl" />
        </set>

        <!-- ValueTypeProperty isValue -->
        <property name="Description" 
                  column="`Description`" 
                  optimistic-lock="false"/>
<!-- ObjectReferenceProperty -->
        <many-to-one name="Method" column="`fk_Method`" class="Kistl.App.Base.MethodNHibernateImpl+MethodProxy, Kistl.Objects.NHibernateImpl" />

<!-- ObjectReferenceProperty -->
        <many-to-one name="Property" column="`fk_Property`" class="Kistl.App.Base.PropertyNHibernateImpl+PropertyProxy, Kistl.Objects.NHibernateImpl" />


        <!-- define the subclasses -->

    </class>
</hibernate-mapping>