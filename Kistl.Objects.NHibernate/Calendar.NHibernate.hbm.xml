<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   default-cascade="save-update" >
    <class name="Kistl.App.Calendar.CalendarNHibernateImpl+CalendarProxy, Kistl.Objects.NHibernateImpl"
           proxy="Kistl.App.Calendar.CalendarNHibernateImpl+CalendarProxy, Kistl.Objects.NHibernateImpl"
           schema="`calendar`"
           table="`Calendars`"
           abstract="false"
           dynamic-update="true"
           optimistic-lock="dirty">

        <id name="ID" column="`ID`" type="Int32" unsaved-value="0">
            <generator class="native">
                <param name="schema">`calendar`</param>
                <param name="sequence">`Calendars_ID_seq`</param>
            </generator>
        </id>

        <!-- define the properties -->
<!-- ObjectReferenceProperty -->
        <many-to-one name="BaseCalendar" column="`fk_BaseCalendar`" class="Kistl.App.Calendar.CalendarNHibernateImpl+CalendarProxy, Kistl.Objects.NHibernateImpl" />

<!-- ObjectReferenceProperty -->
        <set name="CalendarRules" batch-size="100" inverse="true" >
            <key column="`fk_Calendar`" />
            <one-to-many class="Kistl.App.Calendar.CalendarRuleNHibernateImpl+CalendarRuleProxy, Kistl.Objects.NHibernateImpl" />
        </set>

<!-- ObjectReferenceProperty -->
        <many-to-one name="ChangedBy" column="`fk_ChangedBy`" class="Kistl.App.Base.IdentityNHibernateImpl+IdentityProxy, Kistl.Objects.NHibernateImpl" />

        <!-- ValueTypeProperty isValue -->
        <property name="ChangedOn" type="Timestamp"
                  column="`ChangedOn`" 
                  optimistic-lock="true"/>
<!-- ObjectReferenceProperty -->
        <set name="ChildCalendar" batch-size="100" inverse="true" >
            <key column="`fk_BaseCalendar`" />
            <one-to-many class="Kistl.App.Calendar.CalendarNHibernateImpl+CalendarProxy, Kistl.Objects.NHibernateImpl" />
        </set>

<!-- ObjectReferenceProperty -->
        <many-to-one name="CreatedBy" column="`fk_CreatedBy`" class="Kistl.App.Base.IdentityNHibernateImpl+IdentityProxy, Kistl.Objects.NHibernateImpl" />

        <!-- ValueTypeProperty isValue -->
        <property name="CreatedOn" type="Timestamp"
                  column="`CreatedOn`" 
                  optimistic-lock="false"/>
        <!-- ValueTypeProperty isValue -->
        <property name="ExportGuid" 
                  column="`ExportGuid`" 
                  optimistic-lock="false"/>
<!-- ObjectReferenceProperty -->
        <many-to-one name="Module" column="`fk_Module`" class="Kistl.App.Base.ModuleNHibernateImpl+ModuleProxy, Kistl.Objects.NHibernateImpl" />

        <!-- ValueTypeProperty isValue -->
        <property name="Name" 
                  column="`Name`" 
                  optimistic-lock="false"/>

        <!-- define the subclasses -->

    </class>
</hibernate-mapping>