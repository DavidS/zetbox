<?xml version="1.0" encoding="utf-8" ?>
<KistlConfig xmlns="http://dasz.at/Kistl/">
  <ConfigName>Kistl.DalProvider.Ef's Test Configuration</ConfigName>
  <Server StartServer="false">
    <SchemaProvider>MSSQL</SchemaProvider>
    <DatabaseProvider>System.Data.SqlClient</DatabaseProvider>
    <ConnectionString>Data Source=.\SQLEXPRESS;Initial Catalog=Kistl_test;Integrated Security=True;MultipleActiveResultSets=true;</ConnectionString>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <LogFilePath>bin\LogFiles</LogFilePath>
    <CodeGenWorkingPath>bin\CodeGenNext</CodeGenWorkingPath>
    <CodeGenOutputPath>bin\CodeGen</CodeGenOutputPath>
    <CodeGenArchivePath>Backup</CodeGenArchivePath>
    <Modules>
      <Module>Kistl.App.Projekte.Server.CustomServerActionsModule, Kistl.App.Projekte.Server</Module>
      <Module>Kistl.App.Projekte.Common.CustomCommonActionsModule, Kistl.App.Projekte.Common</Module>
    </Modules>
  </Server>
  <SourceFileLocation>
    <!-- default probe path -->
    <string>P:\Kistl\bin\CodeGen\bin\Debug</string>
    <!-- common libaries -->
    <string>P:\Kistl\bin\Debug\bin\Common</string>
    <!-- server libaries -->
    <string>P:\Kistl\bin\Debug\bin\Server</string>
    <!-- Fallback path for local development -->
    <string>P:\Kistl\bin\Debug\bin\Server\fallback</string>

    <!-- CC.net on srv2008 -->
    <!-- default probe path -->
    <string>F:\CruiseControl.Net\Projects\Kistl\bin\CodeGen\bin\Debug</string>
    <!-- common libaries -->
    <string>F:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Common</string>
    <!-- server libaries -->
    <string>F:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Server</string>
    <!-- Fallback path for local development -->
    <string>F:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Server\fallback</string>
  </SourceFileLocation>
</KistlConfig>