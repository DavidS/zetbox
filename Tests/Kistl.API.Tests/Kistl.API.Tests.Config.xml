<?xml version="1.0" encoding="utf-8" ?>
<!-- This config is used as test for the alternate config file loading code path -->
<KistlConfig xmlns="http://dasz.at/Kistl/">
  <ConfigName>Kistl.API.Tests' Configuration</ConfigName>
  <Server StartServer="false">
    <DatabaseProvider>System.Data.SqlClient</DatabaseProvider>
    <ConnectionString>Data Source=.\SQLEXPRESS;Initial Catalog=Kistl_test;Integrated Security=True;MultipleActiveResultSets=true;</ConnectionString>
    <CodeGenPath>bin\CodeGen</CodeGenPath>
    <LogFilePath>bin\LogFiles</LogFilePath>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <KistlDataContextType>Kistl.DALProvider.EF.KistlDataContext, Kistl.DALProvider.EF</KistlDataContextType>
    <ServerObjectHandlerType>Kistl.DALProvider.EF.ServerObjectHandler`1, Kistl.DALProvider.EF</ServerObjectHandlerType>
    <ServerObjectSetHandlerType>Kistl.DALProvider.EF.ServerObjectSetHandler, Kistl.DALProvider.EF</ServerObjectSetHandlerType>
    <ServerCollectionHandlerType>Kistl.DALProvider.EF.ServerCollectionHandler`4, Kistl.DALProvider.EF</ServerCollectionHandlerType>
    <IdentityProviderType>Kistl.Server.WCFIdentityProvider, Kistl.Server</IdentityProviderType>
  </Server>
  <Client StartClient="true" ThrowErrors="true">
    <DocumentStore>DocumentStore\Client\</DocumentStore>
  </Client>
  <SourceFileLocation>
    <!-- default probe path -->
    <string>P:\Kistl\bin\CodeGen\bin\Debug</string>
    <!-- Fallback path for local development -->
    <string>P:\Kistl\bin\Debug\bin\server\fallback</string>
    <!-- default probe path for CC.net on srv2008 -->
    <string>f:\CruiseControl.NET\Projects\Kistl\bin\CodeGen\bin\Debug</string>
    <!-- Fallback path for CC.net on srv2008 -->
    <string>f:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Server\Fallback</string>
    <!-- All other assemblies -->
    <string>P:\Kistl\bin\Debug\bin\server</string>
    <string>P:\Kistl\bin\Debug\bin\client</string>
    <string>f:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Server</string>
    <string>f:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Client</string>
  </SourceFileLocation>
</KistlConfig>