<Application
    x:Class="Kistl.Client.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:Kistl.Client.WPF.Converter"
    StartupUri="MainWindow.xaml"
    Startup="Application_Startup"
    >
    <Application.Resources>
        <converter:IconConverter x:Key="IconConverter" />
        <converter:FormattingConverter x:Key="FormattingConverter" />
        <converter:GetTypeConverter x:Key="GetTypeConverter" />        
    </Application.Resources>
</Application>
