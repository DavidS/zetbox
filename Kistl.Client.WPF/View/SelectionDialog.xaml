<Window x:Class="Kistl.Client.WPF.View.SelectionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Choose Object">
    <DockPanel>
        <Button DockPanel.Dock="Bottom" Click="ChooseClickHandler">Choose!</Button>
        <ListBox x:Name="choicesList" ItemsSource="{Binding Choices}" />
    </DockPanel>
</Window>
