<UserControl x:Class="Kistl.Client.WPF.View.ModuleEditor.ObjectClassInstanceList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:moduleEditor="clr-namespace:Kistl.Client.WPF.View.ModuleEditor"
    xmlns:client="clr-namespace:Kistl.Client.Presentables;assembly=Kistl.Client"
    xmlns:view="clr-namespace:Kistl.Client.WPF.View">
    <DockPanel>
        <CheckBox DockPanel.Dock="Top" IsChecked="{Binding OnlyBaseClasses}" Content="Only base classes" />
        <moduleEditor:InstanceListDisplay DataContext="{Binding}"/>
    </DockPanel>
</UserControl>
