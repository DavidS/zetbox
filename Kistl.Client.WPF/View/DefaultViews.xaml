<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ctrls="clr-namespace:Kistl.Client.WPF.CustomControls"
                    xmlns:view="clr-namespace:Kistl.Client.WPF.View"
                    xmlns:client="clr-namespace:Kistl.Client.PresenterModel;assembly=Kistl.Client">

    <DataTemplate DataType="{x:Type client:DataObjectModel}">
        <view:DataObjectView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type client:BoolPropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type client:DateTimePropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type client:DoublePropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type client:IntPropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type client:StringPropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type client:ReferencePropertyModel}">
        <view:PropertyTextBoxView />
    </DataTemplate>
</ResourceDictionary>