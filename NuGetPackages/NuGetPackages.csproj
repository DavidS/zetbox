<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProductVersion>8.0.30703</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{D3F1D77D-D57E-4A21-A6D6-A42DDC14FB82}</ProjectGuid>
    <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\</SolutionDir>
    <RestorePackages>true</RestorePackages>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OutputPath>..\bin\Debug\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OutputPath>..\bin\Release\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="README.zetbox.txt" />
    <Content Include="zetboxBasic.nuspec" />
    <Content Include="zetboxBasic-All.nuspec" />
    <Content Include="zetboxBasic-EF.nuspec" />
    <Content Include="zetboxBasic-NH.nuspec" />
    <Content Include="zetboxBasic-Tests.nuspec" />
    <Content Include="zetboxBasic-Client.nuspec" />
    <Content Include="zetboxBasic-Common.nuspec" />
    <Content Include="zetboxBasic-Server.nuspec" />
    <Content Include="zetboxBasic-ClientWpf.nuspec" />
    <Content Include="zetboxBasic-ClientASPNET.nuspec" />
    <Content Include="zetboxBasic-WpfExe.nuspec" />
    <Content Include="env.xml">
      <SubType>Designer</SubType>
    </Content>
    <Content Include="Init-zetboxBasic.ps1" />
    <Targets Include="common.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="server.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="client.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="clientwpf.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="clientaspnet.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="wpf.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
    <Targets Include="fallback.targets">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Targets>
  </ItemGroup>
  <ItemGroup>
    <Content Include="install-references.ps1" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <Target Name="ContentCopy">
    <Copy SourceFiles="@(Content)" DestinationFolder="$(OutputPath)" />
    <Copy SourceFiles="@(NuSpec)" DestinationFolder="$(OutputPath)" />
    <Copy SourceFiles="@(Targets)" DestinationFolder="$(OutputPath)" />
  </Target>
  <Target Name="Build" DependsOnTargets="ContentCopy">
  </Target>
  <Target Name="Package" Inputs="@(NuSpec)" Outputs="%(Identity).Dummy" DependsOnTargets="Build">
    <Exec WorkingDirectory="$(OutputPath)" Command="..\$(NuGetCommand) pack @(NuSpec) -prop Configuration=$(Configuration)" />
  </Target>
  <Target Name="Rebuild" DependsOnTargets="Clean;Build">
  </Target>
  <Target Name="Clean">
  </Target>
  <Import Project="$(SolutionDir)\.nuget\nuget.targets" />
</Project>
