<?xml version="1.0" encoding="utf-8" ?>
<KistlConfig xmlns="http://dasz.at/Kistl/">
  <ConfigName>$safesolutionname$ Server Configuration</ConfigName>
  <Server StartServer="true">
    <ConnectionStrings>
      <Database Name="Kistl" Schema="$schema$" Provider="$provider$">$connectionstring$</Database>
    </ConnectionStrings>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <CodeGenWorkingPath>bin\CodeGenNext</CodeGenWorkingPath>
    <CodeGenOutputPath>bin\CodeGen</CodeGenOutputPath>
    <CodeGenArchivePath>Backup</CodeGenArchivePath>
    <Modules>
      <Module>Kistl.API.ApiModule, Kistl.API</Module>
      <Module>Kistl.API.Server.ServerApiModule, Kistl.API.Server</Module>
      <Module>Kistl.API.Common.ApiCommonModule, Kistl.API.Common</Module>
      <Module>Kistl.Server.ServerModule, Kistl.Server</Module>
      <Module>Kistl.Objects.InterfaceModule, Kistl.Objects</Module>
      <Module>Kistl.Objects.$ormapperclassname$Module, Kistl.Objects.$ormapperclassname$Impl</Module>
      <Module>Kistl.Objects.MemoryModule, Kistl.Objects.MemoryImpl</Module>
      <Module>Kistl.DalProvider.$ormapperclassname$.$ormapperclassname$Provider, Kistl.DalProvider.$ormapperclassname$</Module>
      <Module>Kistl.DalProvider.Memory.MemoryProvider, Kistl.DalProvider.Memory</Module>
      <Module>Kistl.App.Projekte.Server.CustomServerActionsModule, Kistl.App.Projekte.Server</Module>
      <Module>Kistl.App.Projekte.Common.CustomCommonActionsModule, Kistl.App.Projekte.Common</Module>
      <!-- <Module>Kistl.API.Server.PerfCounter.ResetOnReadAppender+Module, Kistl.API.Server</Module> -->

      <!-- Development/Master Server modules -->
      <Module>Kistl.Generator.GeneratorModule, Kistl.Generator</Module>
      <Module>Kistl.DalProvider.Memory.Generator.MemoryGeneratorProvider, Kistl.DalProvider.Memory.Generator</Module>
      <Module>Kistl.DalProvider.$ormapperclassname$.Generator.NhGeneratorProvider, Kistl.DalProvider.$ormapperclassname$.Generator</Module>
      <Module>Kistl.DalProvider.Client.Generator.ClientGeneratorProvider, Kistl.DalProvider.Client.Generator</Module>

      <!-- application specific modules -->
      <Module>$safesolutionname$.Server.ServerModule, $safesolutionname$.Server</Module>
    </Modules>
    <ClientFilesLocations>
      <Location Name="Exe">bin\Debug\Kistl.Client.WPF.exe</Location>
      <Location Name="Configs">Configs\Examples\Kistl.Client.WPF.xml</Location>
      <Location Name="Common">bin\Debug\Common</Location>
      <Location Name="Client">bin\Debug\Client</Location>
    </ClientFilesLocations>
  </Server>
  <AssemblySearchPaths>
    <string>Common\Core</string>
    <string>Common\Core.Generated</string>
    <string>Server\Core</string>
    <string>Server\$ormappermodule$</string>
    <string>Server\$ormappermodule$.Generated</string>
    <string>Common\App.ZBox</string>
    <string>Server\App.ZBox</string>
    <string>Common\App.$safesolutionname$</string>
    <string>Server\App.$safesolutionname$</string>
  </AssemblySearchPaths>
</KistlConfig>