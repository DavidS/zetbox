<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
		<Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
		<ProductVersion>8.0.30703</ProductVersion>
		<SchemaVersion>2.0</SchemaVersion>
		<ProjectGuid>$guid1$</ProjectGuid>
		<OutputType>Library</OutputType>
		<AppDesignerFolder>Properties</AppDesignerFolder>
		<RootNamespace>$safeprojectname$</RootNamespace>
		<AssemblyName>$safeprojectname$</AssemblyName>
		<TargetFrameworkVersion>v$targetframeworkversion$</TargetFrameworkVersion>
		<FileAlignment>512</FileAlignment>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
		<DebugSymbols>true</DebugSymbols>
		<DebugType>full</DebugType>
		<Optimize>false</Optimize>
    <OutputPath>..\bin\Debug\Common\App.$safesolutionname$\</OutputPath>
    <KistlOutputPath>..\bin\Debug\</KistlOutputPath>
		<DefineConstants>DEBUG;TRACE</DefineConstants>
		<ErrorReport>prompt</ErrorReport>
		<WarningLevel>4</WarningLevel>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
		<DebugType>pdbonly</DebugType>
 		<Optimize>true</Optimize>
    <OutputPath>..\bin\Release\Common\App.$safesolutionname$\</OutputPath>
    <KistlOutputPath>..\bin\Release\</KistlOutputPath>
		<DefineConstants>TRACE</DefineConstants>
		<ErrorReport>prompt</ErrorReport>
		<WarningLevel>4</WarningLevel>
	</PropertyGroup>
	<ItemGroup>
    <Reference Include="Autofac">
      <HintPath>..\Libs\Kistl\Common\Core\Autofac.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="Kistl.API">
      <HintPath>..\Libs\Kistl\Common\Core\Kistl.API.dll</HintPath>
      <Private>False</Private>
    </Reference>    
    <Reference Include="Kistl.API.Common">
      <HintPath>..\Libs\Kistl\Common\Core\Kistl.API.Common.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="Kistl.Objects">
      <HintPath>..\bin\Debug\Common\Core.Generated\Kistl.Objects.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="log4net">
      <HintPath>..\Libs\Kistl\Common\Core\log4net.dll</HintPath>
      <Private>False</Private>
    </Reference>
		<Reference Include="System"/>
		$if$ ($targetframeworkversion$ >= 3.5)
		<Reference Include="System.Core"/>
		<Reference Include="System.Xml.Linq"/>
		<Reference Include="System.Data.DataSetExtensions"/>
		$endif$
		$if$ ($targetframeworkversion$ >= 4.0)
		<Reference Include="Microsoft.CSharp"/>
 		$endif$
		<Reference Include="System.Data"/>
		<Reference Include="System.Xml"/>
	</ItemGroup>
	<ItemGroup>
		<Compile Include="CommonModule.cs" />
		<Compile Include="Properties\AssemblyInfo.cs" />
	</ItemGroup>
  <ItemGroup>
    <KistlFiles Include="..\Libs\Kistl\**\*.*">
      <InProject>false</InProject>
    </KistlFiles>
    <MyModules Include="..\Modules\**\*.*">
      <InProject>false</InProject>
    </MyModules>
    <MyData Include="..\Data\**\*.*" >
      <InProject>false</InProject>
    </MyData>
  </ItemGroup>
	<Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <!-- To modify your build process, add your task inside one of the targets below and uncomment it. 
       Other similar extension points exist, see Microsoft.Common.targets.  -->
  <Target Name="BeforeBuild">
    <Copy
      SourceFiles="@(KistlFiles)"
      DestinationFiles="@(KistlFiles->'$(KistlOutputPath)\%(RecursiveDir)%(Filename)%(Extension)')" />
    <Copy
      SourceFiles="@(MyModules)"
      DestinationFiles="@(MyModules->'$(KistlOutputPath)\Modules\%(RecursiveDir)%(Filename)%(Extension)')" />
    <Copy
      SourceFiles="@(MyData)"
      DestinationFiles="@(MyData->'$(KistlOutputPath)\Data\%(RecursiveDir)%(Filename)%(Extension)')" />
  </Target>
  <Target Name="AfterBuild">
  </Target>
 </Project>
