<?xml version="1.0" encoding="Windows-1252"?>
<ZetboxConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dasz.at/Zetbox/">
  <ConfigName>$safesolutionname$ Client Configuration</ConfigName>
  <Client>
    <Culture>de-AT</Culture>
    <UICulture>de-AT</UICulture>
    <Modules>
      <Module>Zetbox.Client.ClientModule, Zetbox.Client</Module>
      <Module>Zetbox.API.Client.WcfClientModule, Zetbox.API.Client</Module>
      <Module>Zetbox.Client.WPF.WPFModule, Zetbox.Client.WPF</Module>
      <Module>Zetbox.Microsoft.Module, Zetbox.Microsoft</Module>
      <Module>Zetbox.Microsoft.OutlookMailSender+Module, Zetbox.Microsoft</Module>
      <Module>Zetbox.API.MailProblemReporter+Module, Zetbox.API</Module>
      <Module NotOnFallback="true">$safesolutionname$.Client.ClientModule, $safesolutionname$.Client</Module>
      <Module NotOnFallback="true">$safesolutionname$.Client.WPF.ClientWPFModule, $safesolutionname$.Client.WPF</Module>
    </Modules>
  </Client>
  <AssemblySearchPaths EnableShadowCopy="true">
    <string>Common\Core</string>
    <string>Client\Core</string>
    <string>Client\WPF</string>
    <string>Common\App.Zetbox</string>
    <string>Client\App.Zetbox</string>
    <string>Common\Microsoft</string>
    <string>Common\App.$safesolutionname$</string>
    <string>Client\App.$safesolutionname$</string>
  </AssemblySearchPaths>
</ZetboxConfig>