<Window x:Class="Kistl.Client.Dialogs.ChooseObjectClass"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="ChooseObjectClass" Height="300" Width="300" Loaded="Window_Loaded">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Name="btnChoose" Click="btnChoose_Click">Choose</Button>
            <Button Name="btnCancel" Click="btnCancel_Click">Cancel</Button>
        </ToolBar>
        <ListView x:Name="lst" ItemsSource="{Binding}" 
                MouseDoubleClick="lst_MouseDoubleClick" />
    </DockPanel>
</Window>
