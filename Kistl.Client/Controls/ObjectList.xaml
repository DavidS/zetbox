<UserControl x:Class="Kistl.Client.Controls.ObjectList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    MinHeight="100" MinWidth="200"
    Loaded="ObjectList_Loaded">
    <Grid>
        <ListView x:Name="lst" ItemsSource="{Binding}" 
            MouseDoubleClick="lst_MouseDoubleClick" />
    </Grid>
</UserControl>
