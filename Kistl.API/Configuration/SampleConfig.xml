<?xml version="1.0" encoding="utf-8" ?>
<KistlConfig xmlns="http://dasz.at/Kistl/">
  <ConfigName>Default Server Configuration</ConfigName>
  <Server StartServer="false">
    <DatabaseProvider>System.Data.SqlClient</DatabaseProvider>
    <ConnectionString>Data Source=.\SQLEXPRESS;Initial Catalog=Kistl;Integrated Security=True;MultipleActiveResultSets=true;</ConnectionString>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <LogFilePath>bin\LogFiles</LogFilePath>
    <CodeGenWorkingPath>bin\CodeGenNext</CodeGenWorkingPath>
    <CodeGenOutputPath>bin\CodeGen</CodeGenOutputPath>
    <CodeGenArchivePath>Backup</CodeGenArchivePath>
    <Modules>
      <Module>Kistl.API.ApiModule, Kistl.API</Module>
      <Module>Kistl.API.Server.ServerApiModule, Kistl.API.Server</Module>
      <Module>Kistl.Server.ServerModule, Kistl.Server</Module>
      <Module>Kistl.Objects.InterfaceModule, Kistl.Objects</Module>
      <Module>Kistl.Objects.EFModule, Kistl.Objects.Server</Module>
      <Module>Kistl.Objects.MemoryModule, Kistl.Objects.Memory</Module>
      <Module>Kistl.DalProvider.EF.EfProvider, Kistl.DalProvider.EF</Module>
      <Module>Kistl.DalProvider.Memory.MemoryProvider, Kistl.DalProvider.Memory</Module>
    </Modules>
  </Server>
  <Client StartClient="false" ThrowErrors="true">
    <DocumentStore>DocumentStore\Client\</DocumentStore>
    <Modules>
      <Module>Kistl.API.ApiModule, Kistl.API</Module>
      <Module>Kistl.API.Client.ClientApiModule, Kistl.API.Client</Module>
      <Module>Kistl.Client.ClientModule, Kistl.Client</Module>
      <Module>Kistl.Client.WPF.WPFModule, Kistl.Client.WPF</Module>
      <Module>Kistl.Objects.InterfaceModule, Kistl.Objects</Module>
      <Module>Kistl.Objects.ClientModule, Kistl.Objects.Client</Module>
      <Module>Kistl.Objects.MemoryModule, Kistl.Objects.Memory</Module>
      <Module>Kistl.DalProvider.Client.ClientProvider, Kistl.DalProvider.ClientObjects</Module>
      <Module>Kistl.DalProvider.Memory.MemoryProvider, Kistl.DalProvider.Memory</Module>
    </Modules>
  </Client>
  <SourceFileLocation>
    <!-- default probe path -->
    <string>P:\Kistl\bin\CodeGen\bin\Debug</string>
    <!-- Fallback path for local development -->
    <string>P:\Kistl\bin\Debug\bin\server\fallback</string>
    <!-- Fallback path for CC.net on srv2008 -->
    <string>f:\CruiseControl.Net\Projects\Kistl\bin\Debug\bin\Server\Fallback</string>
  </SourceFileLocation>
</KistlConfig>