        <property name="NEnds_pos" column="`fk_OneEnd_pos`" />

        <!-- ValueTypeProperty isValue -->
        <property name="OtherInt" 
                  column="`OtherInt`" 
                  optimistic-lock="false"/>

        <!-- define the subclasses -->

    </class>
</hibernate-mapping>