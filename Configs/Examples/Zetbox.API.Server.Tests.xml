<?xml version="1.0" encoding="utf-8" ?>
<ZetboxConfig xmlns="http://dasz.at/Zetbox/">
  <ConfigName>Server.API's Test Configuration</ConfigName>
  <Server StartServer="false">
    <ConnectionStrings>
      <Database Name="Zetbox" Schema="SQLCE" Provider="System.Data.SqlServerCe.3.5"></Database>
    </ConnectionStrings>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <CodeGenWorkingPath>bin\CodeGenNext</CodeGenWorkingPath>
    <CodeGenOutputPath>bin\CodeGen</CodeGenOutputPath>
    <CodeGenArchivePath>Backup</CodeGenArchivePath>
  </Server>
  <SourceFileLocation>
    <!-- default probe path -->
    <string>..\..\..\CodeGen\bin\Debug</string>
    <!-- common libaries -->
    <string>..\Common</string>
    <!-- Fallback path for local development -->
    <string>..\Server\fallback</string>
    <!-- All other assemblies -->
    <string>..\Server</string>
  </SourceFileLocation>
</ZetboxConfig>