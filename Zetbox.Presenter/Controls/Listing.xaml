<UserControl x:Class="WPFPresenter.Controls.Listing"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    MinHeight="30" MinWidth="50" Name="ListingCtrl">
    <DockPanel VerticalAlignment="Top">
        <Ellipse Width="15" Height="15" Fill="#FF628FC2" VerticalAlignment="Top" Margin="0,18,20,0" />
        <TextBlock TextWrapping="Wrap" Text="{Binding ElementName=ListingCtrl, Path=Text}" VerticalAlignment="Top" />
    </DockPanel>
</UserControl>
